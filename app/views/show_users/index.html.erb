
<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>First Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.receipt_desc %></td>
        <td><%= user.first_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
