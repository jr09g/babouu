<p id="notice"><%= notice %></p>

<h1>Listing Received Expense Reports</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @received_expense_reports.each do |received_expense_report| %>
      <tr>
        <td><%= link_to received_expense_report.name, received_expense_report %></td>
        <td><%= received_expense_report.status %></td>
        <td><%#= link_to 'Edit', edit_expense_report_path(received_expense_report) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

